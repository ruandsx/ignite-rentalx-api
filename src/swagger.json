{
  "openapi": "3.0.0",
  "info": {
    "title": "RentX Documentation",
    "description": "This is an API for Car Rent",
    "version": "1.0.0"
  },
  "paths": {
    "/categories": {
      "post": {
        "tags": [
          "Category"
        ],
        "summary": "Create Category",
        "description": "Creates a new category",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "Some Category",
                  "description": "Some Description"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "created"
          },
          "500": {
            "description": "error"
          }
        }
      },
      "get": {
        "tags": [
          "Category"
        ],
        "summary": "List Categories",
        "description": "Lists the existing categories",
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}